{
  "expo": {
    "name": "PotholeDetectionApp",
    "slug": "PotholeDetectionApp",
    "platforms": ["ios", "android", "web"],
    "scheme": "potholedetection",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.PotholeDetectionApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.PotholeDetectionApp",
      "permissions": ["CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df0e7fdd-3a98-4aa5-bd4a-92d0760d1869"
      }
    }
  }
}
